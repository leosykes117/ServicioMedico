
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MenuPrincipal.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Mensaje <small>Nombre del Doctor</small>
        </h1>
    </div>
</div>
<!-- /.row -->
<h4>@ViewBag.mensaje</h4>

<div id="alertaReporte" class="alert alert-success alert-dismissable fade in invisible">
    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
    <h4>Reporte del Mes Listo!</h4>
    <p>Doctor <u>Nombre</u>, su reporte del mes esta listo</p>
    <p>
        <a role="button" class="btn btn-primario" href="/Reportes">Ver Reporte</a>
        <a role="button" class="btn btn-warning">Más Tarde</a>
    </p>
</div>

@section scripts{
    <script src="~/Scripts/moment-with-locales.min.js"></script>
    <script src="~/Scripts/scriptsVistas/serializeFormJSON.js"></script>
    <script src="~/Scripts/scriptsVistas/menu.js"></script>
}

